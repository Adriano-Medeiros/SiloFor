<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/borda_item_recycleview_calculos"
    android:paddingStart="16dp"
    android:paddingTop="8dp"
    android:paddingEnd="16dp"
    android:paddingBottom="8dp"
    android:layout_marginTop="15dp">

    <ImageView
        android:id="@+id/imageArticle_item_recycle_titulo"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:adjustViewBounds="true"
        android:importantForAccessibility="no"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/ic_titulo_calculo" />

    <TextView
        android:id="@+id/text_item_recycle_calculo_Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/imageArticle_item_recycle_titulo"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="CÃ¡lculo" />

    <TextView
        android:id="@+id/imageArticle_item_recycle_ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="8dp"
        android:ellipsize="end"
        android:maxLines="4"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageArticle_item_recycle_titulo"
        app:layout_constraintTop_toBottomOf="@+id/text_item_recycle_calculo_Title"
        tools:text="#5" />

    <View
        android:id="@+id/imageArticle_item_recycle_divider"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@color/line_divisor"
        android:backgroundTint="@color/line_divisor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageArticle_item_recycle_titulo"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageArticle_item_recycle_tiposilo"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:adjustViewBounds="true"

        android:importantForAccessibility="no"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageArticle_item_recycle_divider"
        tools:src="@drawable/ic_database" />

    <ImageView
        android:id="@+id/imageArticle_item_recycle_tiposilo2"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginTop="24dp"

        android:layout_marginEnd="8dp"
        android:adjustViewBounds="true"
        android:importantForAccessibility="no"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toStartOf="@+id/text_item_recycle_calculo_tiposilo2"
        app:layout_constraintTop_toBottomOf="@+id/imageArticle_item_recycle_divider"
        tools:src="@drawable/ic_clock" />

    <TextView
        android:id="@+id/text_item_recycle_calculo_tiposilo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:ellipsize="end"
        android:maxLines="4"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageArticle_item_recycle_tiposilo"
        app:layout_constraintTop_toBottomOf="@+id/imageArticle_item_recycle_divider"
        tools:text="Tipo do silo" />

    <TextView
        android:id="@+id/text_item_recycle_calculo_tiposilo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="80dp"
        android:ellipsize="end"
        android:maxLines="4"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageArticle_item_recycle_divider"
        tools:text="Data" />

    <TextView
        android:id="@+id/text_item_recycle_calculo_silo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:ellipsize="end"
        android:maxLines="4"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageArticle_item_recycle_tiposilo"
        tools:text="Cilindrico" />

    <TextView
        android:id="@+id/text_item_recycle_calculo_silo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:ellipsize="end"
        android:maxLines="4"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageArticle_item_recycle_tiposilo2"
        app:layout_constraintTop_toBottomOf="@+id/text_item_recycle_calculo_tiposilo2"
        tools:text="15/01/21" />

</androidx.constraintlayout.widget.ConstraintLayout>